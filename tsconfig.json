{
	"compilerOptions": 
	{
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
		"moduleResolution": "node",
		"resolveJsonModule" : true,
		"module": "CommonJS",
	},
	"include": ["logic/*", "config.ts"]
}